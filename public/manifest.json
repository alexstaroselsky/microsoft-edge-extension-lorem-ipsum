{
  "name": "Lorem Ipsum Generator",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A Chromium Extension to generate lorem ipsum text.",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/build/bundle.js"],
      "css": ["/build/bundle.css", "/global.css"]
    }
  ],
  "web_accessible_resources": ["favicon.png"]
}
